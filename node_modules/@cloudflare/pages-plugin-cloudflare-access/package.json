{
  "name": "@cloudflare/pages-plugin-cloudflare-access",
  "version": "1.0.5",
  "homepage": "https://developers.cloudflare.com/pages/platform/functions/plugins/cloudflare-access/",
  "bugs": {
    "url": "https://github.com/cloudflare/pages-plugins/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudflare/pages-plugins.git",
    "directory": "./packages/cloudflare-access"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./dist/functions/index.js"
    },
    "./api": {
      "types": "./dist/types/api/index.d.ts",
      "default": "./dist/src/api/index.js"
    }
  },
  "main": "./dist/functions/index.js",
  "types": "./index.d.ts",
  "files": [
    "./CHANGELOG.md",
    "./index.d.ts",
    "./dist/"
  ],
  "scripts": {
    "build": "npm run build:src && npm run build:functions",
    "build:functions": "npx wrangler pages functions build --plugin --outdir=./dist/functions --sourcemap --external=@cloudflare/pages-plugin-cloudflare-access/api",
    "prebuild:src": "npm run build:src:types",
    "build:src": "npx esbuild ./src/**/* --outdir=./dist/src --bundle --platform=neutral --format=esm --main-fields=module,browser,main --conditions=workerd --outbase=./src --sourcemap",
    "build:src:types": "npx tsc",
    "prepack": "npm run build"
  },
  "volta": {
    "node": "20.11.0",
    "npm": "10.2.4"
  }
}
